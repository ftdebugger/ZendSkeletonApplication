{{ docType() }}
<html lang="en">
<head>
    <meta charset="utf-8">

    {% block title %}
        {{ headTitle('Skeleton Application') }}
    {% endblock %}

    {% block meta %}
        {% do headMeta('viewport', 'width=device-width, initial-scale=1.0') %}
        {{ headMeta() }}
    {% endblock %}

    {% block link %}
        {{ headLink() }}
    {% endblock %}

    {% block headscript %}
        {{ headScript() }}
    {% endblock %}
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="navbar-inner">
            <div class="container">
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <a class="brand" href="{{ url('home') }}">Skeleton Application</a>
                <div class="nav-collapse collapse">
                    {{ navigation('navigation').menu().setPartial('partial/menu.twig')|raw }}
                </div><!--/.nav-collapse -->
            </div>
        </div>
    </div>
    <div class="container">
        <!-- content|raw is required for phtml fallback -->
        {% block content %}{{ content|raw }}{% endblock content %}
    </div> <!-- /container -->


    {% block inlinescript %}
        {{ inlineScript() }}
    {% endblock %}

    {% block inline %}{% endblock inline %}
</body>
</html>
