{
    "name": "ftdebugger/zend-skeleton-application",
    "description": "Skeleton Application for ZF2 with doctrine, twig and assetic",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Evgeny Shpilevsky",
            "email": "evgeny@shpilevsky.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "framework",
        "zf2",
        "doctrine",
        "twig",
        "assetic"
    ],
    "require": {
        "php":                                  ">=5.4",

        "zendframework/zendframework":          "~2.2.4",
        "doctrine/doctrine-orm-module":         "0.8.*@beta",
        "doctrine/doctrine-module":             "0.8.*@beta",
        "doctrine/cache":                       "v1.0",
        "doctrine/migrations":                  "dev-master",
        "zf-commons/zfc-twig":                  "~1.1.5",
        "zf-commons/zfc-user":                  "~0.1",
        "zf-commons/zfc-user-doctrine-orm":     "~0.1",
        "widmogrod/zf2-assetic-module":         "~1.3",
        "neilime/zf2-twb-bundle":               "dev-master",
        "bjyoungblood/bjy-authorize":           "~1.3",
        "enlitepro/enlite-admin":               "~1.0",
        "enlitepro/enlite-assetic":             "~1.0"
    },
    "require-dev": {
        "zendframework/zftool":                 "dev-master",
        "phpunit/phpunit":                      "~3.7",
        "enlitepro/zf2-scaffold":               "dev-master"
    },
    "suggest": {
        "enlitepro/enlite-behat-extension":     "*"
    },
    "config": {
        "bin-dir": "bin"
    }
}
